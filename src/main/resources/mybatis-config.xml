<?xml version="1.0" encoding="UTF-8"?>
<!-- xml문서의 유효성 체크를 위해 필요한 부분 -->
<!DOCTYPE configuration
  PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-config.dtd">
  
<configuration>
	<typeAliases>
		<typeAlias type="com.kh.dok.member.model.vo.Member" alias="Member"></typeAlias>
		<typeAlias type="com.kh.dok.cinema.model.vo.Cinema" alias="Cinema"></typeAlias>
		<typeAlias type="com.kh.dok.movie.model.vo.Movie" alias="Movie"></typeAlias>
		<typeAlias type="com.kh.dok.area.model.vo.Area" alias="Area"></typeAlias>
	</typeAliases>
	<mappers>
		<mapper resource="mappers/member-mapper.xml"/>
		<mapper resource="mappers/cinema-mapper.xml"/>

		<mapper resource="mappers/admin-mapper.xml"/>

		<mapper resource="mappers/area-mapper.xml"/>

		<mapper resource="mappers/movie-mapper.xml"/>
	</mappers>
</configuration>





















