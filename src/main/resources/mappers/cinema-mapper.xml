<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="Cinema">
	<resultMap type="Cinema" id="cinemaResultSet">
		<id property="theater_id" column="THEATER_ID"/>
		<result property="mid" column="MID"/>
		<result property="theater_name" column="THEATER_NAME"/>
		<result property="theater_root" column="THEATER_ROOT"/>
		<result property="theater_text" column="THEATER_TEXT"/>
	
	</resultMap>
	
<!-- 영화관 추가하기~!~!~!~!~! -->
<insert id="insertTheater" parameterType="Cinema">
		INSERT INTO THEATER VALUES('TT' || TO_CHAR(THEATER_THEATER_ID.NEXTVAL), #{mid},
		#{theaterName}, #{theaterRoot}, #{theaterText})
	</insert>
</mapper>